{
  "extends": ["eslint-config-next", "next/core-web-vitals", "prettier"],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "off",
    "import/order": [
      "error",
      {
        "groups": [
          ["external", "builtin"],
          ["index", "internal", "sibling", "parent"]
        ],
        "newlines-between": "always-and-inside-groups",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
